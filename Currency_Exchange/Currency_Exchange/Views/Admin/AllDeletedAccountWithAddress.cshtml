@model IEnumerable<Application.Dtos.AccountDtos.DeleteAccountAddressDto>

@{
    ViewData["Title"] = "AllDeletedAccountWithAddress";
}

<h1>AllDeletedAccountWithAddress</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AccountId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Balance)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompleteTime)
            </th>
           
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AccountId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Balance)
            </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CompleteTime)
                </td>
          
        </tr>
}
    </tbody>
</table>
