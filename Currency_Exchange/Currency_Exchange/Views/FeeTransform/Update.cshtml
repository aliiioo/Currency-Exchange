@* @model Application.Dtos.CurrencyDtos.UpdateFeeDtos *@

@{
    ViewData["Title"] = "Update";
}

<h1>Update</h1>

<h4>UpdateFeeDtos</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Update">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <input name="feeId" value="@ViewBag.transformFeeId" readonly="readonly" type="hidden" class="form-control" />
            </div>
            <div class="form-group">
                <label class="control-label"> Fee Price</label>
                <input name="feePrice" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
